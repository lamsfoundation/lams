<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<class 
    name="org.lamsfoundation.lams.tool.vote.pojos.VoteSession" 
    table="tl_lavote11_session"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
		    <generator class="native"/>
    </id>

    <property
        name="voteSessionId"
        type="java.lang.Long"
        column="vote_session_id"
        not-null="true"
        length="20"
    >
    </property>

    <property
        name="sessionStartDate"
        type="java.util.Date"
        access="property"
        column="session_start_date"
        length="20"
    />
    
    <property
        name="sessionEndDate"
        type="java.util.Date"
        access="property"
        column="session_end_date"
        length="20"
    />
    
    <property
        name="sessionStatus"
        type="java.lang.String"
        access="property"
        column="session_status"
        length="100"
    />

    <property
        name="session_name"
        type="java.lang.String"
        access="property"
        column="session_name"
        length="100"
    />
    
    <property
        name="voteContentId"
        type="java.lang.Long"
        update="false"
        insert="false"
        access="property"
        column="vote_content_id"
        length="20"
    />

   <many-to-one
        name="voteContent"
        class="org.lamsfoundation.lams.tool.vote.pojos.VoteContent"
        not-null="true"        
        cascade="none"
        outer-join="auto"
        update="true"
        insert="true"
        access="property"
    >
        <column
            name="vote_content_id" 
        />
    </many-to-one>
    
    <set
        name="voteQueUsers"
        lazy="true"
        inverse="true"
        cascade="all-delete-orphan"
        sort="unsorted"
    >
        <key>
        	<column name="vote_session_id" />
   		</key>
        <one-to-many 
            class="org.lamsfoundation.lams.tool.vote.pojos.VoteQueUsr"
        />
    </set>
    
    <many-to-one
        name="groupLeader"
        class="org.lamsfoundation.lams.tool.vote.pojos.VoteQueUsr"
        not-null="true"        
        cascade="all"
        outer-join="auto"
        update="true"
        insert="true"
        access="property"
    >
        <column
            name="group_leader_uid" 
        />
    </many-to-one>
    

</class>
</hibernate-mapping>
