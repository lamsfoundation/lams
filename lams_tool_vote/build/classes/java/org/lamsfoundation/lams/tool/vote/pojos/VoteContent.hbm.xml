<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.vote.pojos.VoteContent" 
    table="tl_lavote11_content"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
        <generator class="native" />
    </id>

	<property
        name="voteContentId"
        type="java.lang.Long"
        column="content_id"
        not-null="true"
        length="20"
    >
    </property>
    
    <property
        name="title"
        type="java.lang.String"
        access="property"
        column="title"
        length="65535"
    />

    <property
        name="instructions"
        type="java.lang.String"
        access="property"        
        column="instructions"
        length="65535"
    />

    <property
        name="reflectionSubject"
        type="java.lang.String"
        access="property"        
        column="reflectionSubject"
        length="65535"
    />
    
    
    
    <property
        name="creationDate"
        type="java.util.Date"
        access="property"        
        column="creation_date"
        length="20"
    />


    <property
        name="updateDate"
        type="java.util.Date"
        access="property"        
        column="update_date"
        length="20"
    />
    
    <property
        name="useSelectLeaderToolOuput"
        type="boolean"
        access="property"
        column="use_select_leader_tool_ouput"
        length="1"        
   />

   <property
        name="reflect"
        type="boolean"
        access="property"
        column="reflect"
        length="1"        
   />
   
   <property
        name="allowText"
        type="boolean"
        access="property"
        column="allowText"
        length="1"        
   />

   
    <property
        name="createdBy"
        type="long"
        access="property"        
        column="created_by"
        length="20"
    />

   <property
        name="defineLater"
        type="boolean"
        access="property"
        column="define_later"
        length="1"        
   />


   <property
        name="lockOnFinish"
        type="boolean"
        access="property"
        column="lock_on_finish"
        length="1"        
   />

    <property
        name="maxNominationCount"
        type="java.lang.String"
        access="property"        
        column="maxNominationCount"
        length="5"
    />
    
    <property
        name="minNominationCount"
        type="java.lang.String"
        access="property"        
        column="minNominationCount"
        length="5"
    />

   <property
        name="showResults"
        type="boolean"
        access="property"
        column="show_results"
        length="1"        
   />
   
   <property
        name="assignedDataFlowObject"
        type="boolean"
        access="property"
        column="assigned_data_flow_object"
        length="1"        
   />
   
  
   <property
        name="maxExternalInputs"
        type="java.lang.Short"
        access="property"
        column="max_external_inputs"
   />
   
   <property
        name="externalInputsAdded"
        type="java.lang.Short"
        access="property"
        column="external_inputs_added"
   />

   <property
        name="submissionDeadline"
        type="java.util.Date"
        access="property"
        column="submission_deadline"
        length="20"        
   />   

    	 <set
            name="voteQueContents"
            order-by="display_order"
            lazy="true"            
            inverse="true"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
              <key
                  column="vote_content_id"
              >
              </key>

              <one-to-many
                  class="org.lamsfoundation.lams.tool.vote.pojos.VoteQueContent"
              />
    </set>


	<set
            name="voteSessions"
            lazy="true"
            inverse="true"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
              <key
                  column="vote_content_id"
              >
              </key>

              <one-to-many
                  class="org.lamsfoundation.lams.tool.vote.pojos.VoteSession"
              />

    </set>
	
</class>
</hibernate-mapping>
