<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.vote.pojos.VoteQueUsr" 
    table="tl_lavote11_usr"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
		    <generator class="native"/>
    </id>

	<property
        name="queUsrId"
        type="java.lang.Long"
        column="user_id"
        not-null="true"
        length="20"
    >
    </property>


    <property
        name="username"
        type="java.lang.String"
        column="username"
        length="100"
    >
    </property>

    <property
        name="fullname"
        type="java.lang.String"
        column="fullname"
        length="100"
    >
    </property>
    
    
   <property
        name="responseFinalised"
        type="boolean"
        access="property"
        column="responseFinalised"
        length="1"        
   />

   <property
        name="finalScreenRequested"
        type="boolean"
        access="property"
        column="finalScreenRequested"
        length="1"        
   />



    <property
        name="voteSessionId"
        type="java.lang.Long"
        update="false"
        insert="false"
        access="property"
        column="vote_session_id"
        length="20"
    >
    </property>


    <many-to-one
        name="voteSession"
        class="org.lamsfoundation.lams.tool.vote.pojos.VoteSession"
        not-null="true"        
        cascade="none"
        outer-join="auto"
        update="true"
        insert="true"
        access="property"
    >
        <column name="vote_session_id" />
    </many-to-one>

   	 <set
            name="voteUsrAttempts"
            lazy="true"
            inverse="true"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
              <key
                  column="que_usr_id"
              >
              </key>

              <one-to-many
                  class="org.lamsfoundation.lams.tool.vote.pojos.VoteUsrAttempt"
              />
    </set>

</class>
</hibernate-mapping>
