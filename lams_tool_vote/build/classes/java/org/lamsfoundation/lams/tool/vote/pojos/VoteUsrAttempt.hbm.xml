<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.vote.pojos.VoteUsrAttempt" 
    table="tl_lavote11_usr_attempt"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
        <generator class="native" />
    </id>

    <property
        name="attemptTime"
        type="java.util.Date"
        access="property"
        column="attempt_time"
        length="20"
    >
    </property>
    
    <property
        name="timeZone"
        type="java.lang.String"
        access="property"
        column="time_zone"
        length="255"
    >
    </property>

 	<property
        name="queUsrId"
        type="java.lang.Long"
        column="que_usr_id"
        not-null="true"
   	    update="false"
	    insert="false"
        length="20"
    >
    </property>
    
    <property
        name="userEntry"
        type="java.lang.String"
        access="property"
        column="userEntry"
        length="255"
    >
    </property>
    
   <property
        name="singleUserEntry"
        type="boolean"
        access="property"
        column="singleUserEntry"
        length="1"        
   />
    
   <property
        name="visible"
        type="boolean"
        access="property"
        column="visible"
        length="1"        
   />

    <many-to-one
        name="voteQueContent"
        class="org.lamsfoundation.lams.tool.vote.pojos.VoteQueContent"
        not-null="true"        
   	    cascade="none"
	    outer-join="auto"
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="vote_nomination_content_id" />
    </many-to-one>
    
    <many-to-one
        name="voteQueUsr"
        class="org.lamsfoundation.lams.tool.vote.pojos.VoteQueUsr"
        not-null="true"        
	    cascade="none"
	    outer-join="auto"
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="que_usr_id" />
    </many-to-one>

</class>
</hibernate-mapping>
