<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.lamsfoundation.lams.tool.vote.pojos.VoteQueContent" 
    table="tl_lavote11_nomination_content"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
        <generator class="native" />
    </id>
    
    <property
        name="nomination"
        type="java.lang.String"
        column="question"
        length="65535"
    >
    </property>
    
    <property
        name="displayOrder"
        type="java.lang.Integer"
        column="display_order"
        length="5"
    >
    </property>
    
    
    <property
        name="voteContentId"
        type="java.lang.Long"
        column="vote_content_id"
        not-null="true"
   	    update="false"
	    insert="false"
        length="20"
    >
    </property>
    
    <many-to-one
        name="voteContent"
        class="org.lamsfoundation.lams.tool.vote.pojos.VoteContent"
        not-null="true"        
	    cascade="none"
	    outer-join="auto"
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="vote_content_id" />
    </many-to-one>
    
    
    <set
        name="voteUsrAttempts"
        lazy="true"
        inverse="true"
		cascade="all-delete-orphan"
	>

        <key>
            <column name="vote_que_content_id" />
        </key>
        <one-to-many 
            class="org.lamsfoundation.lams.tool.vote.pojos.VoteUsrAttempt"
        />
    </set>
    
</class>
</hibernate-mapping>
