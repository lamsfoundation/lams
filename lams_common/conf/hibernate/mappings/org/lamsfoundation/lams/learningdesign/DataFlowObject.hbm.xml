<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>

    <class
        name="org.lamsfoundation.lams.learningdesign.DataFlowObject"
        table="lams_data_flow"
    >

        <id
            name="dataFlowObjectId"
            column="data_flow_object_id"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-DataFlowObject.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="dataTransition"
            class="org.lamsfoundation.lams.learningdesign.DataTransition"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
        >
            <column
                name="transition_id" 
            />
        </many-to-one>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
            length="255"
        />

        <property
            name="displayName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="display_name"
            length="255"
        />

        <property
            name="orderId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="order_id"
            length="11"
        />

        <property
            name="toolAssigmentId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="tool_assigment_id"
            length="11"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-DataFlowObject.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>