<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.lamsfoundation.lams.tool.mc.pojos.McQueContent" 
    table="tl_lamc11_que_content"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
        <generator class="native" />
    </id>
    
    <property
        name="question"
        type="java.lang.String"
        access="property"                
        column="question"
        length="65535"
    >
    </property>
    
    
    <property
        name="displayOrder"
        type="java.lang.Integer"
        column="display_order"
        length="5"
    >
    </property>


    <property
        name="mark"
        type="java.lang.Integer"
        column="mark"
        length="5"
    >
    </property>
    
    <property
	        name="feedback"
	        type="java.lang.String"
	        access="property"
	        column="feedback"
	        length="65535"
	    />

    <property
        name="mcContentId"
        type="java.lang.Long"
        column="mc_content_id"
        not-null="true"
   	    update="false"
	    insert="false"
        length="20"
    >
    </property>
    
    <many-to-one
        name="mcContent"
        class="org.lamsfoundation.lams.tool.mc.pojos.McContent"
        not-null="true"        
	    cascade="none"
	    
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="mc_content_id" />
    </many-to-one>
    
    <set
        name="mcOptionsContents"
        lazy="true"
        inverse="false"
		cascade="all"
		order-by="displayOrder"
    >
        <key>
            <column name="mc_que_content_id" />
        </key>
        <one-to-many 
            class="org.lamsfoundation.lams.tool.mc.pojos.McOptsContent"
        />
    </set>
    
</class>
</hibernate-mapping>
