<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.mc.pojos.McUsrAttempt" 
    table="tl_lamc11_usr_attempt"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
        <generator class="native" />
    </id>

    <property
        name="attemptTime"
        type="java.util.Date"
        access="property"
        column="attempt_time"
        length="20"
    >
    </property>
    
    <property
        name="mark"
        type="java.lang.Integer"
        access="property"        
        column="mark"
    />
    

    <property
        name="passed"
        type="boolean"
        access="property"        
        column="passed"
        length="1"
    />
    
    <property
        name="attemptCorrect"
        type="boolean"
        access="property"        
        column="isAttemptCorrect"
        length="1"
    />
     
     
 	<property
        name="mcQueContentId"
        type="java.lang.Long"
        column="mc_que_content_id"
        not-null="true"
   	    update="false"
	    insert="false"
        length="20"
    >
    </property>
     

 	<property
        name="queUsrId"
        type="java.lang.Long"
        column="que_usr_id"
        not-null="true"
   	    update="false"
	    insert="false"
        length="20"
    >
    </property>

    <many-to-one
        name="mcQueContent"
        class="org.lamsfoundation.lams.tool.mc.pojos.McQueContent"
        not-null="true"        
   	    cascade="none"
	    
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="mc_que_content_id" />
    </many-to-one>
    
    <many-to-one
        name="mcQueUsr"
        class="org.lamsfoundation.lams.tool.mc.pojos.McQueUsr"
        not-null="true"        
	    cascade="none"
	    
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="que_usr_id" />
    </many-to-one>
    
    <many-to-one
        name="mcOptionsContent"
        class="org.lamsfoundation.lams.tool.mc.pojos.McOptsContent"
        not-null="true"                
	    cascade="none"
	    
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="mc_que_option_id" />
    </many-to-one>
    
    <property
        name="confidenceLevel"
        type="int"
        update="true"
        insert="true"
        column="confidence_level"
    />

</class>
</hibernate-mapping>
