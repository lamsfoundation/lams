<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.qa.QaUsrResp" 
    table="tl_laqa11_usr_resp"
>
    <id
        name="responseId"
        type="java.lang.Long"
        column="response_id"
    >
		    <generator class="native"/>
    </id>

    <property
        name="answer"
        type="java.lang.String"
        column="answer"
        length="65535"
    >
    </property>
    
    <property
        name="answerAutosaved"
        type="java.lang.String"
        column="answer_autosaved"
        length="65535"
    >
    </property>
    
    <property
        name="attemptTime"
        type="java.util.Date"
        column="attempt_time"
        length="10"
    >
    </property>
    
   <property
        name="visible"
        type="boolean"
        access="property"
        column="visible"
        length="1"        
   />

    <property
        name="timezone"
        type="java.lang.String"
        column="time_zone"
        length="255"
    >
    </property>

   <many-to-one
            name="qaQuestion"
            class="org.lamsfoundation.lams.tool.qa.QaQueContent"
            not-null="true"            
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
        >
            <column
                name="qa_que_content_id" 
            />
   </many-to-one>


  <many-to-one
            name="qaQueUser"
            class="org.lamsfoundation.lams.tool.qa.QaQueUsr"
            not-null="true"            
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
        >
            <column
                name="que_usr_id" 
            />
  </many-to-one>

</class>
</hibernate-mapping>
