<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>

    <class
        name="org.lamsfoundation.lams.tool.commonCartridge.model.CommonCartridgeSession"
        table="tl_laimsc11_session"
    >

        <id
            name="uid"
            column="uid"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-CommonCartridgeSession.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="sessionEndDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="session_end_date"
        />

        <property
            name="sessionStartDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="session_start_date"
        />

        <property
            name="status"
            type="int"
            update="true"
            insert="true"
            column="status"
        />

        <many-to-one
            name="commonCartridge"
            class="org.lamsfoundation.lams.tool.commonCartridge.model.CommonCartridge"
            cascade="none"
            
            update="true"
            insert="true"
            column="commonCartridge_uid"
        />

        <property
            name="sessionId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="session_id"
        />

        <property
            name="sessionName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="session_name"
            length="250"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-CommonCartridgeSession.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>